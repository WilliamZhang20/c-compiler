// EXPECT: 67
// Comprehensive test of __attribute__ features

// Test packed struct
struct __attribute__((packed)) PackedPoint {
    char flag;
    int x;
    int y;
};

// Test aligned global
int __attribute__((aligned(16))) aligned_data = 30;

// Test section attribute
int __attribute__((section(".mydata"))) section_data = 20;

// Test always_inline
int __attribute__((always_inline)) multiply(int a, int b) {
    return a * b;
}

int main() {
    struct PackedPoint p;
    p.flag = 1;
    p.x = 5;
    p.y = 10;
    
    int result = p.flag + p.x + p.y;  // 16
    result = result + aligned_data;    // 16 + 30 = 46
    result = result + section_data;    // 46 + 20 = 66
    
    // Use multiply function
    int check = multiply(2, 3);  // 6, not added to result
    
    result = result + 1;  // 67
    
    return result;
}
