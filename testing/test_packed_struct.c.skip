// EXPECT: 9
// Test __attribute__((packed)) on structs
// Packed struct should have size 9 (1+4+4), normal struct would be 12 (with padding)

struct __attribute__((packed)) PackedData {
    char flag;
    int x;
    int y;
};

int main() {
    struct PackedData data;
    data.flag = 1;
    data.x = 3;
    data.y = 5;
    return data.flag + data.x + data.y;
}
